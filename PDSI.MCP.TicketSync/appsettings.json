{
	"TicketSync": {
		"Jobs": [
			{
				"Name": "UpdateSmarterTrackVTigerAccounts",
				"Enabled": false
			},
			{
				"Name": "UpdateSmarterTrackRacktablesObjects",
				"Enabled": false
			},
			{
				"Name": "GenerateSmarterTrackCommentLinks",
				"Enabled": true
			}
		]
	},
	"Serilog": {
		"Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog.Sinks.Graylog" ],
		"MinimumLevel": {
			"Default": "Verbose"
		},
		"WriteTo": [
			{ "Name": "Console" },
			{
				"Name": "File",
				"Args": {
					"path": "TicketSync.Log",
					"rollingInterval": "Day",
					"retainedFileCountLimit": 90,
					"restrictedToMinimumLevel": "Verbose"
				}
			},
			{
				"Name": "Graylog",
				"Args": {
					"hostnameOrAddress": "graylog.pdsint.net",
					"port": "12201",
					"transportType": "Udp",
					"minimumLogEventLevel": "Debug"
				}
			}
		]
	}
}